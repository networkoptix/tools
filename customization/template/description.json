            "clientBundleUpgradeCode": "%desktop.windows.clientBundleUpgradeCode%",
            "fullBundleUpgradeCode": "%desktop.windows.fullBundleUpgradeCode%"
        },

        "linux": {
            "companyName": "%desktop.linux.companyId%"
        },

        "macos": {
            "bundleIdentifier": "%desktop.macos.bundleIdentifier%",
