{
    "id": "%id%",
    "brand": "%brand%",
    "companyName": "%companyName%",
    "vmsName": "%vmsName%",
    "skin": "%skin%",
    "uriProtocol": "%uriProtocol%",
    "installerName": "%installerName%",
    "cloudName": "%cloudName%",

    "contact": {
        "companyUrl": "%contact.companyUrl%",
        "supportChannel": "%contact.supportChannel%",
        "supportAddress": "%contact.supportAddress%",
