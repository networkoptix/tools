<plist version="1.0">
  <dict>
	<key>Label</key>
	<string>sysctl</string>
	<key>ProgramArguments</key>
	<array>
	  <string>/usr/sbin/sysctl</string>
	  <string>-w</string>
	  <string>kern.corefile=%N.core.%P</string>
